<view class="body"><view class="head"><view class="head_back"><image src="/static/img/back.png" mode data-event-opts="{{[['tap',[['navigateBack']]]]}}" bindtap="__e"></image></view><view class="head_center hei_38_bold">{{title}}</view><view class=" head_right hei_30_bold"><text data-event-opts="{{[['tap',[['add',['$event']]]]]}}" bindtap="__e">新增</text></view></view><view class="zi_body "><block s-for="{{jiaoyu_list}}" s-for-item="item" s-for-index="index"><block s-if="{{title=='教育背景'}}"><view data-event-opts="{{[['tap',[['jiaoyu_xq',[index]]]]]}}" class="list" bindtap="__e"><view class="list_left"><view class="hei_26 time">{{item.starttime+"-"+item.endtime}}</view><view class="hei_28">{{item.school}}</view></view><image src="/static/lsimg/go_r.png" mode></image></view></block></block><block s-for="{{gongzuo_list}}" s-for-item="item" s-for-index="index"><block s-if="{{title=='工作经历'}}"><view data-event-opts="{{[['tap',[['gongzuo_xq',['$0'],[[['gongzuo_list','',index]]]]]]]}}" class="list" bindtap="__e"><view class="list_left"><view class="hei_26 time">{{item.starttime+"-"+item.endtime}}</view><view class="hei_28">{{item.school+"-"+item.zhuanye}}</view></view><image src="/static/lsimg/go_r.png" mode></image></view></block></block><block s-for="{{shehui_list}}" s-for-item="item" s-for-index="index"><block s-if="{{title=='社会职务'}}"><view data-event-opts="{{[['tap',[['zhiwu_xq',['$0','$1','$2','$3'],[[['shehui_list','',index,'lyinfoid']],[['shehui_list','',index,'zwstate']],[['shehui_list','',index,'school']],[['shehui_list','',index,'zhuanye']]]]]]]}}" class="list" bindtap="__e"><view class="list_left"><view class="hei_26 time">{{item.zwstate==1?'曾任':'现任'}}</view><view class="hei_28">{{item.school+"-"+item.zhuanye}}</view></view><image src="/static/lsimg/go_r.png" mode></image></view></block></block><block s-for="{{rongyu_list}}" s-for-item="item" s-for-index="index"><block s-if="{{title=='荣誉奖励'}}"><view data-event-opts="{{[['tap',[['rongyu_xq',['$0','$1','$2'],[[['rongyu_list','',index,'lyinfoid']],[['rongyu_list','',index,'starttime']],[['rongyu_list','',index,'zhuanye']]]]]]]}}" class="list" bindtap="__e"><view class="list_left"><view class="hei_26 time">{{item.starttime}}</view><view class="hei_28">{{item.zhuanye}}</view></view><image src="/static/lsimg/go_r.png" mode></image></view></block></block><block s-for="{{meiti_list}}" s-for-item="item" s-for-index="index"><block s-if="{{title=='媒体报道'}}"><view data-event-opts="{{[['tap',[['meiti_xq',['$0','$1','$2','$3'],[[['meiti_list','',index,'lyinfoid']],[['meiti_list','',index,'pintai']],[['meiti_list','',index,'starttime']],[['meiti_list','',index,'zhuanye']]]]]]]}}" class="list" bindtap="__e"><view class="list_left"><view class="hei_26 time">{{item.starttime}}</view><view class="hei_28">{{item.zhuanye}}</view></view><image src="/static/lsimg/go_r.png" mode></image></view></block></block><block s-for="{{xueshu_list}}" s-for-item="item" s-for-index="index"><block s-if="{{title=='学术成果'}}"><view data-event-opts="{{[['tap',[['xueshu_xq',['$0','$1','$2','$3','$4','$5'],[[['xueshu_list','',index,'lyinfoid']],[['xueshu_list','',index,'leixing']],[['xueshu_list','',index,'lianjie']],[['xueshu_list','',index,'pintai']],[['xueshu_list','',index,'starttime']],[['xueshu_list','',index,'zhuanye']]]]]]]}}" class="list" bindtap="__e"><view class="list_left"><view class="hei_26 time">{{item.starttime}}</view><view class="hei_28">{{item.zhuanye}}</view></view><image src="/static/lsimg/go_r.png" mode></image></view></block></block></view></view>