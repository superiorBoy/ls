<view class="body"><view class="head"><view class="head_back"><image src="/static/img/back.png" mode data-event-opts="{{[['tap',[['navigateBack']]]]}}" bindtap="__e"></image></view><view class="head_center hei_38_bold">学术成果</view><view class=" head_right hei_30_bold"><text data-event-opts="{{[['tap',[['save',['$event']]]]]}}" bindtap="__e">保存</text></view></view><view class="zi_body shiming_body"><view class="shiming_list hei_28"><view class="shiming_list_left">成果类型</view><input type="text" value="{{wen_leixing}}" data-event-opts="{{[['input',[['__set_model',['','wen_leixing','$event',[]]]]]]}}" bindinput="__e"/></view><view class="shiming_list hei_28"><view class="shiming_list_left">文章链接</view><input type="text" value="{{wen_lianjie}}" data-event-opts="{{[['input',[['__set_model',['','wen_lianjie','$event',[]]]]]]}}" bindinput="__e"/></view><view class="shiming_list hei_28"><view class="shiming_list_left">发表平台</view><input type="text" value="{{wen_pingtai}}" data-event-opts="{{[['input',[['__set_model',['','wen_pingtai','$event',[]]]]]]}}" bindinput="__e"/></view><view class="shiming_list hei_28"><view class="shiming_list_left">时间</view><picker mode="date" data-event-opts="{{[['change',[['time_change',['$event']]]]]}}" bindchange="__e"><view class="{{['zhuanchang_xuan',time==''?'qian_26':'hei_26']}}">{{(time==''?'请选择':time)+''}}<image src="/static/lsimg/go_r.png" mode></image></view></picker></view><view class="jianjie hei_28">成果<textarea class="jianjie_txt" value="{{jianjie_txt}}" placeholder="请填写具体学术成果内容" maxlength="5000" data-event-opts="{{[['input',[['__set_model',['','jianjie_txt','$event',[]]]]]]}}" bindinput="__e"></textarea></view></view></view>