.ani_start {
  -webkit-animation: animationShow 0.4s;
          animation: animationShow 0.4s;
}
.ani_end {
  -webkit-animation: animationHide 0.4s;
          animation: animationHide 0.4s;
}
@-webkit-keyframes animationShow {
from {
    background-color: rgba(0, 0, 0, 0);
    opacity: 0;
}
to {
    background-color: rgba(0, 0, 0, 0.55);
    opacity: 1;
}
}
@keyframes animationShow {
from {
    background-color: rgba(0, 0, 0, 0);
    opacity: 0;
}
to {
    background-color: rgba(0, 0, 0, 0.55);
    opacity: 1;
}
}
@-webkit-keyframes animationHide {
from {
    background-color: rgba(0, 0, 0, 0.55);
    opacity: 1;
}
to {
    background-color: rgba(0, 0, 0, 0);
    opacity: 0;
}
}
@keyframes animationHide {
from {
    background-color: rgba(0, 0, 0, 0.55);
    opacity: 1;
}
to {
    background-color: rgba(0, 0, 0, 0);
    opacity: 0;
}
}
