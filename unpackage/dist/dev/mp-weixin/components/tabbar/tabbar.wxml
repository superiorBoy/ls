<view class="tabBar"><block wx:for="{{tabBar}}" wx:for-item="item" wx:for-index="index" wx:key="url"><view data-event-opts="{{[['tap',[['navTo',['$0',index],[[['tabBar','url',item.url]]]]]]]}}" class="{{['tabbar_item',(item.url==currentPage)?'active':'']}}" bindtap="__e"><block wx:if="{{item.url==currentPage}}"><image src="{{item.imgClick}}" mode></image></block><block wx:else><image src="{{item.imgNormal}}" mode></image></block><view class="text">{{item.text}}</view></view></block></view>