<view class="body">
	<view class="head">
		<view class="head_back">
			<image src="{{img_url}}/uniapp/img/back.png" mode data-event-opts="{{[['tap',[['navigateBack']]]]}}" bindtap="__e"></image>
		</view>
		<view class="head_center hei_38_bold">收藏</view>
		<view class=" head_right"></view>
	</view>
	<view class="zi_body ">
		<view class="jingxuan_body ">
			<block wx:for="{{shoucang_list}}" wx:for-item="item" wx:for-index="__i0__">
				<view class="shangxue_list">
					<view class="shangxue_item">
						<view data-event-opts="{{[['tap',[['go_xq',['$0'],[[['shoucang_list','',__i0__,'consultid']]]]]]]}}" class="shangxue_item_left" bindtap="__e">
							<image src="{{img_url}}/uniapp/img/shangxue1.png" mode></image>
						</view>
						<view class="shangxue_item_right">
							<view class="shangxue_item_title hei_28_bold"><text class="shoucang_wenzhang bai_22">咨询</text></view>
							<view data-event-opts="{{[['tap',[['go_xq',['$0'],[[['shoucang_list','',__i0__,'consultid']]]]]]]}}" class="shangxue_item_body hui_26" bindtap="__e">{{item.information}}</view>
							<view class="quxiao qian_24">
								<view class="quxiao_left hei_24"></view>
								<view data-event-opts="{{[['tap',[['quxiao',['$0'],[[['shoucang_list','',__i0__,'consultid']]]]]]]}}" class="quxiao_btn" bindtap="__e">取消收藏</view>
							</view>
						</view>
					</view>
				</view>
			</block>
		</view>
	</view>
</view>