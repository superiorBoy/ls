<view class="body">
	<view class="head">
		<view class="head_back">
			<image src="{{img_url}}/uniapp/img/back.png" mode data-event-opts="{{[['tap',[['navigateBack']]]]}}" bindtap="__e"></image>
		</view>
		<view class="head_center hei_38_bold">付费电话</view>
		<view class=" head_right"></view>
	</view>
	<view class="zi_body ">
		<view class="dianhua_num hui_26">手机号码：<input class="hei_24" type="text" value="{{dianhua_num}}" data-event-opts="{{[['input',[['__set_model',['','dianhua_num','$event',[]]]]]]}}" bindinput="__e" /></view>
		<view class="shuru">
			<view class="shuru_box"><textarea class="hei_26" value="{{shuru_txt}}" placeholder="请输入您想要问的问题..." maxlength="5000" data-event-opts="{{[['input',[['__set_model',['','shuru_txt','$event',[]]],['bianhua',['$event']]]]]}}" bindinput="__e"></textarea>
				<view class="qian_24 jisuan_num">
					<picker mode="selector" range="{{fenlei_arry}}" range-key="typename" data-event-opts="{{[['change',[['jiating_change',['$event']]]]]}}" bindchange="__e">
						<view class="{{['zhuanchang_xuan',fenlei=='选择类别'?'qian_26':'hei_26']}}">{{fenlei+''}}</view>
					</picker><text>{{yishu_num+"/2000"}}</text>
				</view>
			</view>
		</view>
		<view class="dizhi qian_24" id="allmap">
			<picker-address class="aaaaaa" bind:change="__e" vue-id="7b5b6544-1" data-event-opts="{{[['^change',[['change']]]]}}" bind:__l="__l" vue-slots="{{['default']}}">
				<image src="{{img_url}}/uniapp/img/xuandizhi.png" mode></image>{{''+dizhi+''}}
			</picker-address>
		</view>
		<view class="tiwen_bottom"><button class="bai_30" type data-event-opts="{{[['tap',[['tijiao',['$event']]]]]}}" bindtap="__e">{{"提交订单"+lvshi.phoneprice+"元"}}</button>
			<view class="qian_24">
				<checkbox checked="{{isCheck}}" data-event-opts="{{[['tap',[['checkBox',['$event']]]]]}}" bindtap="__e"></checkbox>我已阅读并同意<text class="lv_24">《小虎网络服务协议》</text>
			</view>
		</view>
	</view>
	<view style="width:0;height:0;display:none;" id="allmap" class="_div"></view>
</view>