.shipin_dibu {
	position: fixed;
	height: 100rpx;
	bottom: 00;
	width: 100%;
	padding: 0 0 0 30rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	box-sizing: border-box;
	background-color: #ffffff;
	box-shadow: 0rpx 0rpx 13rpx 0rpx rgba(0, 0, 0, 0.13);
}
.shipin_dibu_left {
	display: -webkit-box;
