<view class="body">
	<view class="head">
		<view class="head_back">
			<image src="{{img_url}}/uniapp/img/back.png" mode data-event-opts="{{[['tap',[['navigateBack']]]]}}" bindtap="__e"></image>
		</view>
		<view class="head_center hei_38_bold">帮助中心</view>
		<view class="head_right hei_30_bold"></view>
	</view>
	<view class="zi_body">
		<view class="help_top hei_24">
			<block wx:for="{{leibie}}" wx:for-item="item" wx:for-index="__i0__">
				<view data-event-opts="{{[['tap',[['dalei',['$0'],[[['leibie','',__i0__,'kindid']]]]]]]}}" class="help_item" bindtap="__e">
					<image src="{{img_url+item.pic}}" mode></image>
					<view>{{''+item.kindname+''}}</view>
				</view>
			</block>
		</view>
		<view class="help_txt qian_28">常见问题</view>
		<block wx:for="{{wenti_list}}" wx:for-item="item" wx:for-index="__i1__">
			<view data-event-opts="{{[['tap',[['xq',['$0','$1'],[[['wenti_list','',__i1__,'title']],[['wenti_list','',__i1__,'information']]]]]]]}}" class="help_list hui_28" bindtap="__e">
				<image src="{{img_url}}/uniapp/img/wenhao.png" mode></image>{{''+item.title+''}}
			</view>
		</block>
	</view>
</view>