
.head {
	border-bottom: 2rpx solid #f7f7f7;
}
.tishi_name {
	text-align: center;
	margin: 67rpx 0 70px;
}
.tishi_name image {
	width: 467rpx;
}
.tishi_tu {
	text-align: center;
}
.tishi_tu image {
	width: 600rpx;
}
.tishi_txt {
	font-size: 36rpx;
	text-align: center;
	color: #00a16b;
	line-height: 68rpx;
	font-weight: bold;
}
.down{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	margin-top: 80rpx;
}
.down button{
		width: 330rpx;
		height: 80rpx;
		background-color: #0eb77e;
		-webkit-border-radius: 40rpx;
		        border-radius: 40rpx;
		line-height: 80rpx;
}
.ios,.Android{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
}
.ios image{
		width: 27rpx;
		height: 32rpx;
		margin-right: 18rpx;
}
.Android image{
		width: 33rpx;
		height: 32rpx;
		margin-right: 12rpx;
}

