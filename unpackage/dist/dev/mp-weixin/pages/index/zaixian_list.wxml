<view>
	<view class="head">
		<view class="head_back">
			<image src="{{img_url}}/uniapp/img/back.png" mode data-event-opts="{{[['tap',[['navigateBack']]]]}}" bindtap="__e"></image>
		</view>
		<view class="head_center hei_38_bold">在线咨询</view>
		<view class=" head_right hei_30_bold"></view>
	</view>
	<view class="zi_body">
		<view class="xiaoxi_list">
			<block wx:for="{{$root.l0}}" wx:for-item="item" wx:for-index="__i0__">
				<block wx:if="{{item.$orig.user_to}}">
					<view data-event-opts="{{[['tap',[['kan',['$0'],[[['xiaoxi_list','',__i0__,'user_to.userid']]]]]]]}}" class="xiaoxi_item" bindtap="__e">
						<view class="xiaoxi_item_left">
							<view class="xiaoxi_tx">
								<image src="{{img_url+item.$orig.user_to.photourl}}" mode></image>
								<block wx:if="{{item.$orig.messagecount>0}}"><text class="xiaoxi_num bai_20">{{item.$orig.messagecount}}</text></block>
							</view>
							<view class="xiaoxi_item_left_name">
								<view class="hei_30_bold xiaoxi_item_name">{{''+item.$orig.opposite_nickname+''}}</view>
								<view class="qian_26 txt_over">
									<block wx:if="{{item.$orig.msgtype==1}}">
										<view>
											<u-parse vue-id="{{'58dd7093-1-'+__i0__}}" content="{{item.m0}}" bind:__l="__l"></u-parse>
										</view>
									</block>
									<block wx:if="{{item.$orig.msgtype==2}}">
										<view>[图片]</view>
									</block>
								</view>
							</view>
						</view>
						<view class="xiaoxi_item_right qian_20">{{''+item.f0+''}}</view>
					</view>
				</block>
			</block>
		</view>
	</view>
	<view class="padding_bottom"></view>
</view>