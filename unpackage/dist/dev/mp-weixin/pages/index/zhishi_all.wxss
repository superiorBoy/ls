
page {
	background-color: #f7f7f7;
}
.zhishi_item {
	text-align: center;
	height: 136rpx;
	width: 25%;
	border-right: 2rpx solid #f7f7f7;
	border-bottom: 2rpx solid #f7f7f7;
	padding-top: 20rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
}
.zhishi_item image {
	width: 34rpx;
	height: 34rpx;
	margin-bottom: 10rpx;
}
.zhishi_list {
	background-color: #ffffff;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-flex-wrap: wrap;
	        flex-wrap: wrap;
	margin-bottom: 20rpx;
}
.fenlei_item {
	background-color: #ffffff;
	margin-bottom: 20rpx;
}
.fenlei_title {
	height: 82rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	padding: 0 30rpx;
	border-bottom: 2rpx solid #f7f7f7;
}
.fenlei_title image {
	width: 34rpx;
	height: 34rpx;
	margin-right: 14rpx;
}
.xiaolei {
	padding: 30rpx 0 10rpx;
}
.xiaolei text {
	margin: 0 0 20rpx 25rpx;
	display: inline-block;
	border: solid 1rpx #dcdcdc;
	height: 44rpx;
	line-height: 44rpx;
	padding: 0 25rpx;
	min-width: 160rpx;
	text-align: center;
	-webkit-border-radius: 10rpx;
	        border-radius: 10rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
}

