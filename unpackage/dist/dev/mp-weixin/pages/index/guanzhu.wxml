<view class="body">
	<view class="head">
		<view class="head_back">
			<image src="{{img_url}}/uniapp/img/back.png" mode data-event-opts="{{[['tap',[['navigateBack']]]]}}" bindtap="__e"></image>
		</view>
		<view class="head_center hei_38_bold">关注</view>
		<view class=" head_right"></view>
	</view>
	<view class="zi_body ">
		<view class="fensi_body">
			<block wx:for="{{jilu_list}}" wx:for-item="item" wx:for-index="__i0__">
				<view class="fensi_list">
					<view class="fensi_list_left">
						<view data-event-opts="{{[['tap',[['go_zhuye',['$0'],[[['jilu_list','',__i0__,'userid']]]]]]]}}" class="fensi_list_left_tx" bindtap="__e">
							<image class="fensi_list_tx" src="{{img_url+item.photourl}}" mode></image>
							<image class="fensi_list_ren" src="{{img_url}}/uniapp/img/renzheng2.png" mode></image>
						</view>
						<view class="fensi_list_left_r">
							<view data-event-opts="{{[['tap',[['go_zhuye',['$0'],[[['jilu_list','',__i0__,'userid']]]]]]]}}" class="hei_26 fensi_list_name" bindtap="__e">{{item.nickname}}</view>
							<view class="hui_24">{{item.fensi+"个粉丝"}}</view>
						</view>
					</view>
					<view class="fensi_list_right yiguan qian_26">已关注</view>
				</view>
			</block>
		</view>
	</view>
</view>