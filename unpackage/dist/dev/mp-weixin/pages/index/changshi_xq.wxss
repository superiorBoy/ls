	        box-sizing: border-box;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	height: 108rpx;
	background-color: #FFFFFF;
	-webkit-box-shadow: -1rpx -6rpx 10rpx 0rpx 
			rgba(158, 158, 158, 0.38);
	        box-shadow: -1rpx -6rpx 10rpx 0rpx 
			rgba(158, 158, 158, 0.38);
}
