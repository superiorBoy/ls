
page {
	background-color: #f7f7f7;
}
.head {
	border-bottom: 2rpx solid #e8e8e8;
}
.pay_top {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	padding: 30rpx;
	background-color: #ffffff;
	-webkit-box-align: end;
	-webkit-align-items: flex-end;
	        align-items: flex-end;
}
.pay_top_left {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
}
.pay_top image {
	width: 110rpx;
	height: 110rpx;
	margin-left: 30rpx;
	margin-right: 39rpx;
}
.jiage {
	margin-top: 26rpx;
}
.pay_top_name {
	padding-top: 6rpx;
}
.jiejue {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	height: 65rpx;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	padding: 0 30rpx;
}
.jiejue_wenti {
	margin-right: 55rpx;
}
.jiejue_wenti image,
.jiejue_manyi image {
	width: 22rpx;
	height: 22rpx;
	margin-right: 14rpx;
}
.pay_neirong {
	background-color: #ffffff;
	padding: 30rpx 30rpx 35rpx;
	border-bottom: 2rpx solid #f7f7f7;
}
.pay_neirong_list {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	margin-bottom: 16rpx;
}
.pay_neirong_list image {
	width: 26rpx;
	height: 26rpx;
	margin-right: 15rpx;
}
.pay_neirong_txt {
	margin-bottom: 26rpx;
}
.pay_liucheng {
	padding: 0 30rpx 30rpx;
	background-color: #ffffff;
}
.pay_liucheng_title {
	padding: 30rpx 0 24rpx;
}
.pay_liucheng_txt {
	line-height: 40rpx;
}
.chakan {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	margin-top: 20rpx;
}
.chakan_list {
	margin-right: 20rpx;
}
.chakan_list image {
	width: 58rpx;
	height: 58rpx;
	margin-left: -10rpx;
}
.chakan_list image:first-child {
	margin-left: 0;
}
.pay_bottom {
	padding: 0 30rpx;
	margin: 85rpx 0 60rpx;
	text-align: center;
}
.weixin_pay {
	height: 88rpx;
	background-color: #0eb77e;
	border-radius: 44rpx;
	line-height: 88rpx;
	margin-bottom: 16rpx;
	text-align: left;
	padding-left: 190rpx;
}
.weixin_pay image {
	width: 36rpx;
	height: 31rpx;
	margin-right: 10rpx;
}
.zhifubao_pay {
	height: 88rpx;
	background-color: #0078ff;
	border-radius: 44rpx;
	line-height: 88rpx;
	margin-bottom: 20rpx;
	vertical-align: middle;
	text-align: left;
	padding-left: 190rpx;
}
.zhifubao_pay image {
	width: 39rpx;
	height: 39rpx;
	margin-right: 10rpx;
	vertical-align: middle;
}
.tan_jiage {
	width: 691rpx;
	background-color: #ffffff;
	border-radius: 8rpx;
}
.tan_title {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: end;
	-webkit-justify-content: flex-end;
	        justify-content: flex-end;
	padding: 0 38rpx;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	height: 78rpx;
}
.tan_title image {
	width: 26rpx;
	height: 26rpx;
}
.feiyong_title {
	text-align: center;
	margin-bottom: 68rpx;
}
.tan_bottom {
	padding: 0 24rpx 100rpx;
}
.tan_bottom view {
	margin-bottom: 20rpx;
	line-height: 40rpx;
}

