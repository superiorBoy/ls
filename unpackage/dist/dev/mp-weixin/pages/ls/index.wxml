<view>
	<view class="head">
		<view class="head_back">
			<image src="{{img_url}}/uniapp/img/back.png" mode data-event-opts="{{[['tap',[['navigateBack']]]]}}" bindtap="__e"></image>
		</view>
		<view class="head_center hei_38_bold">接单</view>
		<view class="head_right"></view>
	</view>
	<view class="zi_body">
		<view class="jiedan_tab qian_30_bold">
			<block wx:for="{{tabs}}" wx:for-item="tab" wx:for-index="index">
				<view data-event-opts="{{[['tap',[['jiedan_qiehuan',[index]]]]]}}" class="{{['jiedan_tab_item',index==active?'jiedan_tab_item_active':'']}}" bindtap="__e">{{''+tab+''}}</view>
			</block>
		</view>
		<view class="jiedan_list">
			<block wx:for="{{$root.l0}}" wx:for-item="item" wx:for-index="__i0__">
				<view class="jiedan_item">
					<view class="jiedan_item_left">
						<image src="{{img_url+item.$orig.photourl}}" mode></image>
						<view class="jiedan_item_left_name">
							<view class="hei_26 jiedan_item_name">{{''+item.$orig.phone+''}}</view>
							<view class="qian_22">{{''+item.f0+''}}</view>
						</view>
					</view>
					<view class="jiedan_item_right hong_26">
						<block wx:if="{{active==0}}">
							<view>{{''+item.$orig.paymoney+"元/"+(item.$orig.zixunshicahng==24?'1天':item.$orig.zixunshicahng==72?'3天':item.$orig.zixunshicahng==720?'1个月':item.$orig.zixunshicahng+'小时')+''}}</view>
						</block>
						<block wx:if="{{active==1}}">
							<view>{{''+item.$orig.paymoney+"元/"+(item.$orig.zixunshicahng==24?'1天':item.$orig.zixunshicahng==72?'3天':item.$orig.zixunshicahng==720?'1个月':item.$orig.zixunshicahng+'分钟')+''}}</view>
						</block>
						<view data-event-opts="{{[['tap',[['jiedan',['$0'],[[['list','',__i0__]]]]]]]}}" class="jiedan_btn" bindtap="__e">接单</view>
					</view>
				</view>
			</block>
		</view>
	</view>
</view>