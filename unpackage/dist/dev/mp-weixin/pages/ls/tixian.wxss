
.head {
	border-bottom: 2rpx solid #E8E8E8;
}
.tixian_body {
	padding: 0 30rpx;
}
.tixian_top {
	height: 237rpx;
	background: url(data:image/png;base64,) no-repeat;
	background-size: 100% 100%;
	text-align: center;
	padding-top: 75rpx;
	box-sizing: border-box;
}
.tixian_top image {
	width: 30rpx;
	height: 28rpx;
	margin-right: 13rpx;
}
.tixian_list {
	padding: 35rpx 0;
	border-bottom: 2rpx solid #E8E8E8;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.tixian_list_left {
	width: 147rpx;
}
.tixian_list input {
	width: 400rpx;
}
.jine {
	font-size: 60rpx;
	color: #FFFFFF;
	font-weight: bold;
}
.all {
	position: absolute;
	right: 30rpx;
}
.tishi {
	margin-top: 10rpx;
	display: inline-block;
}
.tixian_btn {
	height: 88rpx;
	background-color: #f43a51;
	border-radius: 44rpx;
	line-height: 88rpx;
	margin: 150rpx 0 50rpx;
}
.tixian_shuoming_bottom {
	margin-top: 20rpx;
	margin-bottom: 50rpx;
	line-height: 40rpx;
}

