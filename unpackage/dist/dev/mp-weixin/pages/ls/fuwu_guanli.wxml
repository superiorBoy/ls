<view class="body">
	<view class="head">
		<view class="head_back">
			<image src="{{img_url}}/uniapp/img/back.png" mode data-event-opts="{{[['tap',[['navigateBack']]]]}}" bindtap="__e"></image>
		</view>
		<view class="head_center hei_38_bold">服务管理</view>
		<view class=" head_right hei_30_bold"></view>
	</view>
	<view class="zi_body ">
		<view class="fuwu_top">
			<view class="fuwu_list">
				<view class="fuwu_list_left qian_24">
					<image src="{{img_url}}/uniapp/lsimg/ls_guanli1.png" mode></image><text class="hei_30 fuwu_leixing">服务范围</text>
					<block wx:if="{{zhuanchang_arry[lawyerauth.expertise1]&&zhuanchang_arry[lawyerauth.expertise1].shanchangname}}"><text class="youshi">{{zhuanchang_arry[lawyerauth.expertise1].shanchangname}}</text></block>
					<block wx:if="{{zhuanchang_arry[lawyerauth.expertise2]&&zhuanchang_arry[lawyerauth.expertise2].shanchangname}}"><text class="youshi">{{zhuanchang_arry[lawyerauth.expertise2].shanchangname}}</text></block>
					<block wx:if="{{zhuanchang_arry[lawyerauth.expertise3]&&zhuanchang_arry[lawyerauth.expertise3].shanchangname}}"><text class="youshi">{{zhuanchang_arry[lawyerauth.expertise3].shanchangname}}</text></block>
				</view>
				<view class="fuwu_list_right">
					<image src="{{img_url}}/uniapp/lsimg/go_r.png" mode></image>
				</view>
			</view>
			<view class="fuwu_list">
				<view class="fuwu_list_left qian_24">
					<image src="{{img_url}}/uniapp/lsimg/ls_guanli2.png" mode></image><text class="hei_30 fuwu_leixing">服务地区</text>{{''+lawyerauth.province+" "+lawyerauth.city+''}}
				</view>
				<view class="fuwu_list_right">
					<image src="{{img_url}}/uniapp/lsimg/go_r.png" mode></image>
				</view>
			</view>
		</view>
		<view class="fuwu_top">
			<view class="fuwu_list">
				<view class="fuwu_list_left qian_24">
					<image src="{{img_url}}/uniapp/lsimg/ls_guanli3.png" mode></image><text class="hei_30 fuwu_leixing">在线咨询</text>结算金额￥<text class="hong_24">{{jiage.zaixianprice}}</text>{{"/"+jiage.zaixiantime+'小时'}}
				</view>
				<view class="fuwu_list_right">
					<switch name="switch" disabled="{{true}}" checked="{{kaiqi}}" color="#0eb77e" data-event-opts="{{[['tap',[['tanchu',['$event']]]]]}}" bindtap="__e"></switch>
				</view>
			</view>
			<view class="fuwu_list">
				<view class="fuwu_list_left qian_24">
					<image src="{{img_url}}/uniapp/lsimg/ls_guanli4.png" mode></image><text class="hei_30 fuwu_leixing">电话咨询</text>结算金额￥<text class="hong_24">{{jiage.dianhuaprice}}</text>{{"/"+jiage.dianhuatime+'分钟'}}
				</view>
				<view class="fuwu_list_right">
					<switch name="switch" disabled="{{true}}" checked="{{kaiqi}}" color="#0eb77e" data-event-opts="{{[['tap',[['tanchu',['$event']]]]]}}" bindtap="__e"></switch>
				</view>
			</view>
		</view>
		<view class="fuwu_top">
			<navigator class="fuwu_list" url="jiedan_time">
				<view class="fuwu_list_left qian_24">
					<image src="{{img_url}}/uniapp/lsimg/fuwu_jiedan.png" mode></image><text class="hei_30 fuwu_leixing">接单时间</text><text class="qian_24">{{jiedan_time.workstate==1?'全天24小时':jiedan_time.workstate==2?'白天（9:00-23:00）':jiedan_time.workstate==3?'晚上（23:00-9:00）':jiedan_time.workstarttime+'-'+jiedan_time.workendtime}}</text>
				</view>
				<view class="fuwu_list_right qian_24">去设置<image src="{{img_url}}/uniapp/lsimg/go_r.png" mode></image>
				</view>
			</navigator>
		</view>
	</view>
	<block wx:if="{{is_tan}}">
		<view class="bg">
			<view class="tan_tishi">
				<view class="tan_tishi_top hei_30_bold">
					<view class="tan_tishi_top_left hong_30_bold">
						<image src="{{img_url}}/uniapp/lsimg/jinggao.png" mode></image><text>提示</text>
					</view>
					<image class="guanbi" src="{{img_url}}/uniapp/img/tan_close.png" mode data-event-opts="{{[['tap',[['close',['$event']]]]]}}" bindtap="__e"></image>
				</view>
				<view class="hui_24 tan_tishi_txt">必须是 vip会员 才可以开启</view><button class="bai_26" type data-event-opts="{{[['tap',[['close',['$event']]]]]}}" bindtap="__e">知道啦</button>
			</view>
		</view>
	</block>
</view>