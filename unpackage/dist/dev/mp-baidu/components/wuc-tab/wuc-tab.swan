<scroll-view class="{{['wuc-tab',tabClass]}}" style="{{(tabStyle)}}" scroll-with-animation="{{true}}" scroll-x="{{true}}" scroll-left="{{scrollLeft}}"><block s-if="{{!textFlex}}"><view class="_div"><block s-for="{{tabList}}" s-for-item="item" s-for-index="index" s-key="index"><view class="{{['wuc-tab-item _div',index===tabCur?selectClass+' cur':'']}}" id="{{index}}" data-event-opts="{{[['tap',[['tabSelect',[index,'$event']]]]]}}" bindtap="__e"><text class="{{[item.icon]}}"></text><label class="_span">{{item.typename}}</label></view></block></view></block><block s-if="{{textFlex}}"><view class="flex text-center _div"><block s-for="{{tabList}}" s-for-item="item" s-for-index="index" s-key="index"><view class="{{['wuc-tab-item flex-sub _div',index===tabCur?selectClass+' cur':'']}}" id="{{index}}" data-event-opts="{{[['tap',[['tabSelect',[index,'$event']]]]]}}" bindtap="__e"><text class="{{[item.icon]}}"></text><label class="_span">{{item.typename}}</label></view></block></view></block></scroll-view>