<view class="body"><view class="head"><view class="head_back"><image src="{{url}}uniapp/img/back.png" mode data-event-opts="{{[['tap',[['navigateBack']]]]}}" bindtap="__e"></image></view><view class="head_center hei_38_bold">案件委托</view><view class=" head_right"></view></view><view class="tab_top hui_26"><block s-for="{{tab_arry}}" s-for-item="item" s-for-index="index"><text data-event-opts="{{[['tap',[['qiehuan',[index]]]]]}}" class="{{['',index==active?'hong_26_bold':'']}}" bindtap="__e">{{item}}</text></block></view><view class="zi_body tab_zi_body"><view class="tiwen_list"><block s-for="{{$root.l0}}" s-for-item="item" s-for-index="index"><view class="tiwen_item"><view class="tiwen_item_name"><view class="tiwen_item_name_l "><image src="{{img_url+item.$orig.photourl}}" mode></image></view><view class="tiwen_item_name_r"><view class="hui_24 tiwen_item_name_phone">{{item.$orig.mobile+''}}<text style="margin-left:20rpx;"><image src="{{url}}uniapp/lsimg/weituo_dihzi.png" mode></image><text class="qian_20">{{item.$orig.province+"-"+item.$orig.city}}</text></text></view><view class=" qian_22">{{''+item.f0+''}}</view></view><block s-if="{{item.$orig.tiwenstate!=4}}"><view class="xuanshang_zhuangtai"><text class="{{[item.$orig.state==1?'hong_26':'qian_26']}}">{{item.$orig.state==1?'进行中':'已结束'}}</text></view></block></view><view class="tiwen_item_top hui_26">{{item.$orig.information}}</view><block s-if="{{item.$orig.typeid}}"><view class="tiwen_item_center qian_22"><view class="tiwen_item_biaoqian"><image src="{{url}}uniapp/img/biaoqian.png" mode></image>{{''+fenlei[item.$orig.typeid].typename+''}}</view><view class="tiwen_item_dizhi"><image src="{{url}}uniapp/img/dizhi.png" mode></image>{{''+item.$orig.name+(item.$orig.sex==1?'先生':'女士')+''}}</view><view class="tiwen_item_shijian"><image src="{{url}}uniapp/lsimg/chat_dianhua_icon.png" mode></image>{{''+item.$orig.phone+''}}</view></view></block><view class="huifu_btn"><view class="anniu"><block s-if="{{item.$orig.state==1}}"><button class="huifu hong_24" type data-event-opts="{{[['tap',[['go_chat',['$0'],[[['jilu_list','',index,'userid']]]]]]]}}" bindtap="__e">联系TA</button></block><block s-if="{{item.$orig.state==1}}"><button class="huifu hong_24" type data-event-opts="{{[['tap',[['xq',['$0'],[[['jilu_list','',index]]]]]]]}}" bindtap="__e">详情</button></block><block s-if="{{item.$orig.state!=1}}"><button class="huifu huifu_jieshu qian_24" type data-event-opts="{{[['tap',[['tishi']]]]}}" bindtap="__e">详情</button></block></view></view></view></block></view><block s-if="{{jilu_list.length==0}}"><view class="none_list"><image src="{{url}}uniapp/img/none_tiwen.png" mode="widthFix"></image><view class="none_list_txt qian_26">暂无委托记录</view></view></block></view><block s-if="{{is_tan}}"><view class="bg"><view class="tan_tishi"><view class="tan_tishi_top hei_30_bold"><view class="tan_tishi_top_left hong_30_bold"><image src="{{url}}uniapp/lsimg/jinggao.png" mode></image><text>温馨提示</text></view><image class="guanbi" src="{{url}}uniapp/img/tan_close.png" mode data-event-opts="{{[['tap',[['close',['$event']]]]]}}" bindtap="__e"></image></view><view class="hui_24 tan_tishi_txt">此案件已停止委托，去查看进行中的案件</view><button class="bai_26" type data-event-opts="{{[['tap',[['close',['$event']]]]]}}" bindtap="__e">知道啦</button></view></view></block></view>