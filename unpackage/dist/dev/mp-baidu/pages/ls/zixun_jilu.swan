<view class="body"><view class="head"><view class="head_back"><image src="/static/img/back.png" mode data-event-opts="{{[['tap',[['navigateBack']]]]}}" bindtap="__e"></image></view><view class="head_center hei_38_bold">咨询记录</view><view class=" head_right"><image src="/static/lsimg/sousuo.png" mode data-event-opts="{{[['tap',[['tan',['$event']]]]]}}" bindtap="__e"></image></view></view><view class="tab_top hui_26"><block s-for="{{tab_arry}}" s-for-item="item" s-for-index="index"><text data-event-opts="{{[['tap',[['qiehuan',[index]]]]]}}" class="{{['',index==active?'hong_26_bold':'']}}" bindtap="__e">{{item}}</text></block></view><view class="zi_body tab_zi_body"><view class="zixun_list"><block s-for="{{$root.l0}}" s-for-item="item" s-for-index="index"><view class="zixun_item"><view class="zixun_item_top"><view class="zixun_item_left"><view class="zixun_item_tx"><image src="{{img_url+item.$orig.photourl}}" mode></image></view><view class="zixun_item_xinxi"><view class="hei_26">{{item.$orig.mobile}}</view><view class="hong_26 zixun_item_feiyong">{{"￥"+item.$orig.paymoney}}</view><view class="qian_22 zixun_item_feiyong">{{item.f0}}</view></view></view><view class="zixun_item_right"><view class="{{['',item.$orig.zixunstate==4?'qian_26_bold':'hong_26_bold']}}">{{item.$orig.zixunstate==1?'未付款':item.$orig.zixunstate==2?'已付款':item.$orig.zixunstate==3?'接单中':item.$orig.zixunstate==4?'已完成':item.$orig.zixunstate==5?'已退款':item.$orig.zixunstate==6?'已取消':''}}</view></view></view><view class="zixun_item_top_bottom"><block s-if="{{item.$orig.zixunstate==2}}"><view data-event-opts="{{[['tap',[['jiadan',['$0'],[[['jilu_list','',index,'consultid']]]]]]]}}" class="iten_lianxi hong_26" bindtap="__e">立即接单</view></block><block s-if="{{item.$orig.refundstate}}"><view data-event-opts="{{[['tap',[['tui_xq',['$0'],[[['jilu_list','',index]]]]]]]}}" class="iten_lianxi hong_26" bindtap="__e">退款详情</view></block><view data-event-opts="{{[['tap',[['go_chat',['$0'],[[['jilu_list','',index,'userid']]]]]]]}}" class="iten_lianxi hong_26" bindtap="__e"><image src="/static/lsimg/hong_zaixian.png" mode></image>在线联系</view></view></view></block></view><block s-if="{{jilu_list.length==0}}"><view class="none_list"><image src="{{url}}uniapp/img/none_weituo.png" mode="widthFix"></image><view class="none_list_txt qian_26">暂无咨询记录</view></view></block></view><uni-popup class="vue-ref" vue-id="47fe91d3-1" type="bottom" data-ref="popup" vue-slots="{{['default']}}"><view class="tan"><view class="tan_title hei_30">查询条件</view><view class="tan_list hei_26"><view class="tan_list_top"><text>咨询金额</text><input type="text" value="{{name}}" placeholder="0.00" data-event-opts="{{[['input',[['__set_model',['','name','$event',[]]]]]]}}" bindinput="__e"/></view></view><view class="tan_buttom "><button class="quxiao hong_30" type data-event-opts="{{[['tap',[['quxiao_btn',['$event']]]]]}}" bindtap="__e">取消搜索</button><button class="queren bai_30" type data-event-opts="{{[['tap',[['queding_btn',['$event']]]]]}}" bindtap="__e">确认搜索</button></view></view></uni-popup></view>