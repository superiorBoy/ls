<view class="body"><view class="head"><view class="head_back"><image src="{{url}}uniapp/img/back.png" mode data-event-opts="{{[['tap',[['navigateBack']]]]}}" bindtap="__e"></image></view><view class="head_center hei_38_bold">电话咨询</view><view class=" head_right"><image src="{{url}}uniapp/lsimg/sousuo.png" mode data-event-opts="{{[['tap',[['tan',['$event']]]]]}}" bindtap="__e"></image></view></view><view class="tab_top hui_26"><block s-for="{{tab_arry}}" s-for-item="item" s-for-index="index"><text data-event-opts="{{[['tap',[['qiehuan',[index]]]]]}}" class="{{['',index==active?'hong_26_bold':'']}}" bindtap="__e">{{item}}</text></block></view><view class="zi_body tab_zi_body"><view class="zixun_list"><block s-for="{{$root.l0}}" s-for-item="item" s-for-index="index"><view class="zixun_item"><view class="zixun_item_top"><view class="zixun_item_left"><view class="zixun_item_tx"><image src="{{img_url+item.$orig.photourl}}"></image></view><view class="zixun_item_xinxi"><view class="hei_26">{{item.$orig.nickname}}</view><view class="hong_26 zixun_item_feiyong">{{"￥"+item.$orig.paymoney}}</view><view class="qian_22 zixun_item_feiyong">{{item.f0}}</view></view></view><view class="zixun_r"><view class="{{['zixun_leibie',item.$orig.zixunstate==4?'qian_26_bold':'hong_26_bold']}}">{{''+(item.$orig.zixunstate==1?'未付款':item.$orig.zixunstate==2?'已付款':item.$orig.zixunstate==3?'接单中':item.$orig.zixunstate==4?'已完成':item.$orig.zixunstate==5?'已退款':'')+''}}</view><view data-event-opts="{{[['tap',[['call',['$0'],[[['jilu_list','',index,'phone']]]]]]]}}" class="zixun_dianhua" bindtap="__e"><image src="{{url}}uniapp/img/hy_dianhua.png" mode></image>{{''+item.$orig.phone+''}}</view></view></view><view class="zixun_item_top_bottom"><block s-if="{{item.$orig.zixunstate==2}}"><view data-event-opts="{{[['tap',[['jiadan',['$0'],[[['jilu_list','',index,'consultid']]]]]]]}}" class="iten_lianxi hong_26" bindtap="__e">立即接单</view></block><block s-if="{{item.$orig.refundstate}}"><view data-event-opts="{{[