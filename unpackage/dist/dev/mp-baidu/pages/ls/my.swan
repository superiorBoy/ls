<view>
    <view class="my_top">
        <view class="my_top_left">
            <block s-if="{{user}}">
                <image class="ls_tx" src="{{img_url+user.photourl}}" mode
                    data-event-opts="{{[['tap',[['go_geren',['$event']]]]]}}" bindtap="__e"></image>
            </block>
            <block s-if="{{!user}}">
                <image class="ls_tx" src="/static/lsimg/moren_tx.png" mode></image>
            </block>
            <block s-if="{{is_login}}">
                <view class="my_top_left_r">
                    <view data-event-opts="{{[['tap',[['go_geren',['$event']]]]]}}" class="my_name hei_30_bold"
                        bindtap="__e">{{''+(user?user.nickname:'')+'律师'}}<block s-if="{{user}}">
                            <view class="renzheng_xinxi">
                                <block s-if="{{user.isreal==1}}">
                                    <image class="renzheng1" src="{{url}}uniapp/lsimg/yi_renzheng1.png" mode></image>
                                </block>
                                <block s-if="{{user.iszhiye==1}}">
                                    <image class="renzheng2" src="{{url}}uniapp/lsimg/yi_renzheng2.png" mode></image>
                                </block>
                                <block s-if="{{user.iszhiye==1}}">
                                    <image class="renzheng3" src="{{url}}uniapp/lsimg/yi_renzheng3.png" mode></image>
                                </block>
                            </view>
                        </block>
                    </view>
                    <block s-if="{{user&&user.isreal==1}}">
                        <view class="renzheng bai_20">认证律师</view>
                    </block>
                </view>
            </block>
            <block s-if="{{!is_login}}">
                <view class="my_top_weoienglu hei_26">
                    <navigator class="go_denglu" url="login">登录</navigator>
                    <navigator class="go_denglu" url="../index/zhuce?state=2">注册</navigator>
                </view>
            </block>
        </view>
        <view style="position:relative;">
            <view data-event-opts="{{[['tap',[['qiehuan',['$event']]]]]}}" class="my_top_right hei_30_bold"
                bindtap="__e">切换到用户中心</view>
            <view data-event-opts="{{[['tap',[['tiaozhuan',['mingpian']]]]]}}" class="my_mingpian bai_30_bold"
                bindtap="__e">我的名片</view>
        </view>
    </view>
    <view class="my_ziliao">
        <view data-event-opts="{{[['tap',[['tiaozhuan',['fensi']]]]]}}" class="my_ziliao_item" bindtap="__e">
            <view class="my_ziliao_item_top hei_28">{{geshu.fensi}}</view>
            <view class="my_ziliao_item_bottom hei_22">粉丝</view>
        </view>
        <view data-event-opts="{{[['tap',[['tiaozhuan',['tiwen_guanli']]]]]}}" class="my_ziliao_item" bindtap="__e">
            <view class="my_ziliao_item_top hei_28">{{geshu.tiwen}}</view>
            <view class="my_ziliao_item_bottom hei_22">提问</view>
        </view>
        <view data-event-opts="{{[['tap',[['tiaozhuan',['my_jindou']]]]]}}" class="my_ziliao_item" bindtap="__e">
            <view class="my_ziliao_item_top hei_28">{{user.jindou}}</view>
            <view class="my_ziliao_item_bottom hei_22">金豆</view>
        </view>
        <view data-event-opts="{{[['tap',[['tiaozhuan',['tixian']]]]]}}" class="my_ziliao_item" bindtap="__e">
            <view class="my_ziliao_item_top hei_28">{{user.rmb}}</view>
            <view class="my_ziliao_item_bottom hei_22">余额</view>
        </view>
    </view>
    <view class="my_bottom">
        <view data-event-opts="{{[['tap',[['tiaozhuan',['vip']]]]]}}" class="guanggao" bindtap="__e">
            <image src="{{url}}uniapp/lsimg/my_vip.png" mode></image>
        </view>
        <view class="my_guanli ls_guanli">
            <view class="my_title hei_30_bold">律师管理</view>
            <view class="my_list">
                <navigator class="ls_item" url="shiming_renzheng">
                    <image src="{{url}}uniapp/lsimg/my_ls1.png" mode></image>
                    <view class="hui_24 ">实名认证</view>
                </navigator>
                <view data-event-opts="{{[['tap',[['tiaozhuan',['zhiye_renzheng']]]]]}}" class="ls_item" bindtap="__e">
                    <image src="{{url}}uniapp/lsimg/my_ls2.png" mode></image>
                    <view class="hui_24 ">执业认证</view>
                </view>
                <view data-event-opts="{{[['tap',[['tiaozhuan',['ls_renzheng']]]]]}}" class="ls_item" bindtap="__e">
                    <image style="width:50rpx;height:50rpx;" src="{{url}}uniapp/lsimg/my_ls3.png" mode></image>
                    <view class="hui_24 ">律师认证</view>
                </view>
                <view data-event-opts="{{[['tap',[['tiaozhuan',['anli']]]]]}}" class="ls_item" bindtap="__e">
                    <image style="width:48rpx;height:50rpx;" src="{{url}}uniapp/lsimg/my_ls4.png" mode></image>
                    <view class="hui_24">成功案例</view>
                </view>
                <view data-event-opts="{{[['tap',[['tiaozhuan',['zixun_baojia']]]]]}}" class="ls_item" bindtap="__e">
                    <image src="{{url}}uniapp/lsimg/my_ls5.png" mode></image>
                    <view class="hui_24 ">咨询报价</view>
                </view>
                <view data-event-opts="{{[['tap',[['tiaozhuan',['fuwu_guanli']]]]]}}" class="ls_item" bindtap="__e">
                    <image src="{{url}}uniapp/lsimg/my_ls6.png" mode></image>
                    <view class="hui_24 ">服务管理</view>
                </view>
            </view>
        </view>
        <view class="my_guanli zhanghao_guanli">
            <view class="my_title hei_30_bold">账号管理</view>
            <view class="my_list">
                <navigator class="zhanghao_item" url="geren_xinxi">
                    <image style="width:40rpx;height:32rpx;margin-bottom:32rpx;"
                        src="{{url}}uniapp/lsimg/my_zhanghao1.png" mode></image>
                    <view class="hui_24 ">个人信息</view>
                </navigator>
                <view data-event-opts="{{[['tap',[['tiaozhuan',['tixian']]]]]}}" class="zhanghao_item" bindtap="__e">
                    <image style="width:40rpx;height:39rpx;" src="{{url}}uniapp/lsimg/my_zhanghao3.png" mode></image>
                    <view class="hui_24 ">账号提现</view>
                </view>
                <view data-event-opts="{{[['tap',[['tiaozhuan',['my_jindou']]]]]}}" class="zhanghao_item" bindtap="__e">
                    <image style="width:42rpx;height:43rpx;" src="{{url}}uniapp/lsimg/ls_jindou.png" mode></image>
                    <view class="hui_24 " style="position:relative;top:-4rpx;">获取金豆</view>
                </view>
                <view data-event-opts="{{[['tap',[['tiaozhuan',['kefu']]]]]}}" class="zhanghao_item" bindtap="__e">
                    <image style="width:40rpx;height:40rpx;" src="{{url}}uniapp/lsimg/ls_kefu.png" mode></image>
                    <view class="hui_24 ">联系客服</view>
                </view>
                <view data-event-opts="{{[['tap',[['tiaozhuan',['gaimima']]]]]}}" class="zhanghao_item" bindtap="__e">
                    <image style="width:40rpx;height:40rpx;" src="{{url}}uniapp/lsimg/my_zhanghao4.png" mode></image>
                    <view class="hui_24 ">修改密码</view>
                </view>
                <view data-event-opts="{{[['tap',[['tiaozhuan',['vip']]]]]}}" class="zhanghao_item" bindtap="__e">
                    <image style="width:46rpx;height:40rpx;" src="{{url}}uniapp/lsimg/my_zhanghao5.png" mode></image>
                    <view class="hui_24 ">VIP管理</view>
                </view>
                <view data-event-opts="{{[['tap',[['tiaozhuan',['mingpian']]]]]}}" class="zhanghao_item" bindtap="__e">
                    <image style="width:40rpx;height:32rpx;margin-bottom:34rpx;"
                        src="{{url}}uniapp/lsimg/my_zhanghao6.png" mode></image>
                    <view class="hui_24 ">我的名片</view>
                </view>
            </view>
        </view>
    </view>
    <view class="padding_bottom"></view>
    <tab-bar class="vue-ref" vue-id="0268bb2c-1" currentPage="{{currentPage}}" data-ref="ls_mainindex"></tab-bar>
</view>